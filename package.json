{
  "name": "react-swiper",
  "version": "0.0.1",
  "description": "react swiper using swiper.js",
  "scripts": {
    "clean": "del ./dist",
    "clean-src": "del \"./!(node_modules|types|dist)/**/*.+(ts|tsx)\"",
    "test": "jest",
    "snapshot": "jest -u",
    "lint": "tsc --outDir ./dist -d && npm run clean && tslint \"./!(node_modules)/**/*.ts\"",
    "build": "npm run clean && tsc -d --outDir ./dist"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "mobx": "3.1.0",
    "mobx-react": "^4.0.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "swiper": "^4.4.2"
  },
  "author": "codingstar",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "15.6.10",
    "@types/react-dom": "15.5.6",
    "del-cli": "^0.2.1",
    "tslint": "^3.13.0",
    "tslint-microsoft-contrib": "^2.0.9",
    "typescript": "^2.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codingstar/react-swiper.git"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
